            //                                     ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛   ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛

            bindings = <
                     &kp Q       &kp H       &kp O            &kp U        &kp QUESTION            &kp G                &kp C        &kp R         &kp F       &kp Z
                     &mt LGUI Y  &mt LALT R  &mt LCTRL E      &mt LSHFT A  &kp PERIOD              &kp D                &mt RSHFT T  &mt RCTRL S   &mt RALT N  &mt RGUI B
&mt LEFT_WIN ESCAPE  &kp J       &kp X       &kp COMMA        &kp K        &kp SQT                 &kp W                &kp M        &kp L         &kp P       &kp V       &mt RIGHT_WIN C_AL_FILE_BROWSER
                                             &lt 1 BACKSPACE  &lt 2 ENTER  &mt LEFT_SHIFT SPACE    &mt RIGHT_SHIFT TAB  &lt 3 SPACE  &lt 4 DELETE
